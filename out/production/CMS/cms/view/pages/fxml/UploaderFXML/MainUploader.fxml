<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      stylesheets="/cms/view/pages/stylesheets/custom.css, /cms/view/pages/stylesheets/UIStylesheet.css"
      fx:controller="cms.controller.v2controllers.UploaderControllers.MainUploaderController"
      alignment="BASELINE_CENTER" styleClass="outerPane"
>
    <padding><Insets top="50" right="50" bottom="50" left="50"/></padding>

    <Label
            GridPane.columnIndex="0" GridPane.rowIndex="5"
            prefWidth="400" minWidth="400"
            styleClass="h1" text="Master Uploader"></Label>
    <Pane prefWidth="1200" GridPane.columnIndex="1" GridPane.rowIndex="5"></Pane>
    <Button fx:id="backButton" id="backButton" text="Back"
            prefWidth="100" minWidth="100"
            styleClass="button"
            GridPane.columnIndex="2" GridPane.rowIndex="5"
    ></Button>

    <Pane GridPane.columnIndex="0" GridPane.rowIndex="6" minHeight="30" prefHeight="30"></Pane>

    <ScrollPane styleClass="borderedScrollPane"
            GridPane.columnIndex="0" GridPane.rowIndex="7"
            GridPane.columnSpan="3">
        <padding><Insets top="40" right="40" bottom="40" left="40"/></padding>
        <FlowPane fx:id="localDataList" id="localDataList"
              vgap="30" hgap="50" prefWidth="1600"  alignment="BASELINE_CENTER"
        ><padding><Insets top="20" right="20" bottom="20" left="20"/></padding>
        </FlowPane>
    </ScrollPane>

</GridPane>
